      margin-left: 0px;
      padding-top: 0;
      padding-bottom: 0;
      margin-top: 0;
      margin-bottom: 0;
    }

    .col-md-5 {
      padding:0;
      margin:0;
      padding-right: 0px;
      padding-left: 10px;
      margin-right: 0px;
      margin-left: 0px;
      padding-top: 0;
      padding-bottom: 0;
      margin-top: 0;
      margin-bottom: 0;
    }

    .col-md-6 {
      padding:0;
      margin:0;
      padding-right: 0px;
      padding-left: 0px;
      margin-right: 0px;
      margin-left: 0px;
      padding-top: 0;
      padding-bottom: 0;
      margin-top: 0;
      margin-bottom: 0;
    }

    .col-xs-11 {
      padding-left: 10px;
      margin-left: 0px;
    }  

    .col-xs-12 {
      padding-left: 30px;
      margin-left: 0px;
    }    

    .datetime-datepicker {

      height:5

    }

    body.modal-open {
      position: fixed;
    }

